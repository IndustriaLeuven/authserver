<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.oauth.pre_authorization_listener" class="App\EventListener\OAuthPreAuthorizationEventListener">
            <argument type="service" id="app.admin.user.repo" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="app.user_properties_listener" class="App\EventListener\UserPropertiesValidEventListener">
            <argument type="service" id="security.token_storage" />
            <argument type="service" id="app.admin.user.repo" />
            <argument type="service" id="router" />
            <argument type="service" id="braincrafted_bootstrap.flash" />
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
