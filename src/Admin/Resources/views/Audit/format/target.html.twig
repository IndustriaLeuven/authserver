{% spaceless %}
    <a href="{{ url('admin_audit_gets', app.request.query.all()|merge({page:null,target:obj.objectClass})) }}">{{ obj.objectClass }}</a>
    {% if obj.objectId %}
        <b>@</b>
        <a href="{{ url('admin_audit_gets', app.request.query.all()|merge({page:null,target:obj.objectClass~"@"~obj.objectId})) }}">{{ obj.objectId }}</a>
    {% endif %}
{% endspaceless %}
