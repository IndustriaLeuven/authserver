{
    "license": "AGPL",
    "type": "project",
    "description": "Authserver, an OAuth2-based single-signon authentication provider written in PHP",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "symfony/icu": "~1.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "jms/serializer-bundle": "~0.13.0",
        "friendsofsymfony/rest-bundle": "~1.4,>=1.5.1",
        "friendsofsymfony/http-cache-bundle": "~1.0",
        "willdurand/hateoas-bundle": "~0.4",
        "hautelook/templated-uri-bundle": "~2.0",
        "willdurand/rest-extra-bundle": "~1.0",
        "braincrafted/bootstrap-bundle": "~2.1",
        "twbs/bootstrap": "~3.0",
        "components/jquery": "~1.11",
        "doctrine/migrations": "dev-master",
        "fortawesome/font-awesome": "~4.1",
        "knplabs/knp-paginator-bundle": "~2.4",
        "friendsofsymfony/oauth-server-bundle": "dev-master as 1.4.2",
        "doctrine/doctrine-migrations-bundle": "~1.0",
        "ircmaxell/password-compat": "~1.0",
        "kuleuven/shibboleth-bundle": "dev-master",
        "stof/doctrine-extensions-bundle": "dev-master",
        "rollbar/rollbar": "^0.15.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/IndustriaLeuven/ShibbolethBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/vierbergenlars/FOSOAuthServerBundle"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
